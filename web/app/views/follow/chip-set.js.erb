(function () {
	let chipEventElem = document.getElementById('<%= "follow-chip-set__#{@locals[:type]}-#{@locals[:event_id]}" %>');

	setTimeout(function () {
		requestIdleCallback(function (){
      morphdom.default(chipEventElem, '<%= j render(partial: 'components/follow/follow', locals: @locals) %>'.trim(), {
        onBeforeElUpdated: function (fromEl, toEl) {
          return !fromEl.isEqualNode(toEl);
        }
      })
	}, {timeout: 250})
	}, 250)
})()
