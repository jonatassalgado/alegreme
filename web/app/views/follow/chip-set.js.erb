(function () {
	let chipEventElem = document.getElementById('<%= @data[:identifier] %>');

	setTimeout(function () {
		requestIdleCallback(function (){
      morphdom.default(chipEventElem, '<%= j render(partial: 'components/follow/follow', locals: {data: @data}) %>'.trim(), {
        onBeforeElUpdated: function (fromEl, toEl) {
          return !fromEl.isEqualNode(toEl);
        }
      })
	}, {timeout: 250})
	}, 250)
})()
